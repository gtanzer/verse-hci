using semantics ProseSummative.Semantics;
using learners ProseSummative.WitnessFunctions;

language Summative;

@complete feature double Score = ProseSummative.RankingScore;

@input string x;
@start string program := Substring(x, pos);
int? pos := // A position at index 'k' (from the left if k >= 0, or from the right if k < 0)
            AbsPos(x, k)
int k;

